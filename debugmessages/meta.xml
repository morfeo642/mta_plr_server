<!-- Este es un recurso para el juego MTA:SA -->
<meta>
	<info name="debugmessages" author="victor" version="1.0.0" description="Permite visualizr errores, warnings.. de la consola debugscript mediante ventanas de diálogo" />
  <!-- Todos los recursos dependerán del recurso module -->
	<include resource="module" />
	<include resource="guidialog" />
	<!-- Scripts comunes para todos los recursos -->
  <script src="common/server.lua" type="server" />
	<script src="common/client.lua" type="client" />
	<script src="client.lua" type="client" />
	<script src="server.lua" type="server" />
	<!-- Funciones que deben ser exportadas explicitamente para que todos los 
	modulos funcionen -->
	<export function="__set" type="server"/>
	<export function="__get" type="server"/>
	<export function="__set" type="client"/>
	<export function="__get" type="client"/>
</meta>
